# Supabase Configuration
VITE_SUPABASE_URL=https://xyzsupabase.com # عنوان URL لخدمة Supabase التي تستخدمها للتفاعل مع قاعدة البيانات الخاصة بك.
VITE_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9... # المفتاح المجهول (Public Anon Key) الخاص بـ Supabase الذي يتيح الوصول إلى قاعدة البيانات دون الحاجة لمصادقة المستخدم.

# API Keys
VITE_RAPIDAPI_KEY=a123b456c789d0... # مفتاح API للوصول إلى خدمات RapidAPI، مثل خدمات الطقس أو الرياضة.
VITE_OPENAI_API_KEY=sk-ABC123... # مفتاح API الخاص بـ OpenAI للوصول إلى نموذج GPT أو خدمات الذكاء الاصطناعي الأخرى.

# Application Configuration
VITE_APP_URL=http://localhost:5173 # عنوان URL لتطبيقك في بيئة التطوير المحلية.
VITE_API_URL=http://localhost:3000/api # عنوان URL للـ API التي تستخدمها مع التطبيق المحلي.

# Authentication
VITE_JWT_SECRET=your-strong-secret-key # مفتاح سري لتوليد والتحقق من الرموز المميزة (JWT) للمصادقة.
VITE_COOKIE_DOMAIN=localhost # النطاق الذي يتم فيه تخزين ملفات تعريف الارتباط (Cookies) للمصادقة، هنا يتم تخزينها في النطاق المحلي (localhost).

# External Services
VITE_STRIPE_PUBLIC_KEY=pk_test_123... # مفتاح الوصول العام لـ Stripe لتكامل خدمات الدفع.
VITE_CLOUDINARY_CLOUD_NAME=your-cloud # اسم السحابة الخاصة بك على Cloudinary، وهي خدمة لإدارة وتحميل الصور والفيديوهات.
VITE_CLOUDINARY_UPLOAD_PRESET=fitness_uploads # اسم الإعداد المسبق لتحميل الملفات على Cloudinary.

# Email Service (Gmail SMTP)
VITE_EMAIL_SERVICE=smtp # نوع خدمة البريد الإلكتروني، هنا يتم استخدام SMTP لإرسال البريد الإلكتروني.
VITE_EMAIL_HOST=smtp.gmail.com # عنوان خادم SMTP للبريد الإلكتروني (هنا Gmail).
VITE_EMAIL_PORT=587 # المنفذ المستخدم لخدمة البريد الإلكتروني (587 للبريد الآمن).
VITE_EMAIL_USER=your@gmail.com # عنوان البريد الإلكتروني المستخدم لإرسال الرسائل.
VITE_EMAIL_PASSWORD=your-app-specific-password # كلمة المرور الخاصة بالتطبيق لمصادقة البريد الإلكتروني.

# Analytics
VITE_GOOGLE_ANALYTICS_ID=G-ABC123XYZ # معرّف Google Analytics الذي يتم استخدامه لمتابعة وتحليل بيانات حركة المرور على موقعك.
VITE_MIXPANEL_TOKEN=12345abcde... # رمز Mixpanel الذي يتم استخدامه لجمع وتحليل بيانات المستخدمين داخل التطبيق.

# Feature Flags
VITE_ENABLE_CHAT=true # تمكين أو تعطيل ميزة الدردشة في التطبيق.
VITE_ENABLE_AI_FEATURES=true # تمكين أو تعطيل الميزات المدعومة بالذكاء الاصطناعي في التطبيق.
VITE_ENABLE_NOTIFICATIONS=true # تمكين أو تعطيل إشعارات التطبيق.

# Localization
VITE_DEFAULT_LANGUAGE=en # اللغة الافتراضية للتطبيق (هنا هي الإنجليزية).
VITE_SUPPORTED_LANGUAGES=["en","ar"] # اللغات المدعومة من التطبيق، وهنا هما الإنجليزية والعربية.

# Cache Configuration
VITE_CACHE_TTL=3600 # مدة صلاحية الذاكرة المؤقتة (Cache) بالثواني، بعد انقضاء هذه المدة سيتم تحديث البيانات.
VITE_MAX_CACHE_SIZE=100 # الحد الأقصى لحجم الذاكرة المؤقتة.

# Security
VITE_MAX_LOGIN_ATTEMPTS=5 # الحد الأقصى لعدد محاولات تسجيل الدخول قبل حظر المستخدم.
VITE_PASSWORD_MIN_LENGTH=8 # الحد الأدنى لطول كلمة المرور (هنا 8 أحرف).
VITE_SESSION_TIMEOUT=3600 # مدة انتهاء الجلسة بالثواني (هنا 3600 ثانية = 1 ساعة).

# Performance
VITE_IMAGE_QUALITY=80 # جودة الصورة عند تحميلها (من 0 إلى 100).
VITE_MAX_UPLOAD_SIZE=5242880 # الحد الأقصى لحجم الملفات القابلة للتحميل (هنا 5 ميجابايت).

# API Endpoints
VITE_AUTH_API=/api/auth # نقطة النهاية الخاصة بالمصادقة.
VITE_USERS_API=/api/users # نقطة النهاية الخاصة بإدارة المستخدمين.
VITE_WORKOUTS_API=/api/workouts # نقطة النهاية الخاصة بالتمارين.
VITE_NUTRITION_API=/api/nutrition # نقطة النهاية الخاصة بالتغذية.
VITE_CHAT_API=/api/chat # نقطة النهاية الخاصة بالدردشة.
VITE_REPORTS_API=/api/reports # نقطة النهاية الخاصة بالتقارير.
VITE_PAYMENTS_API=/api/payments # نقطة النهاية الخاصة بالمدفوعات.
VITE_NOTIFICATIONS_API=/api/notifications # نقطة النهاية الخاصة بالإشعارات.

# WebSocket Configuration
VITE_WS_URL=ws://localhost:3000 # عنوان URL الخاص بـ WebSocket للتواصل في الوقت الفعلي مع الخادم.

# CDN Configuration
VITE_CDN_URL=https://cdn.yourservice.com # عنوان CDN (شبكة توزيع المحتوى) لتحميل الملفات بشكل أسرع من خلال الخوادم المنتشرة جغرافيًا.
VITE_ASSETS_URL=https://assets.yourservice.com # عنوان لخدمة الأصول (مثل الصور أو الملفات الأخرى).

# Exercise API Configuration
VITE_EXERCISE_API_URL=https://exercisedb.p.rapidapi.com/exercises # عنوان URL الخاص بـ API للتمارين البدنية.
VITE_EXERCISE_API_HOST=exercisedb.p.rapidapi.com # اسم المضيف الخاص بـ API للتمارين البدنية.

# AI Service Configuration
VITE_AI_SERVICE_URL=https://api.openai.com/v1 # عنوان URL الخاص بخدمة OpenAI للوصول إلى نماذج الذكاء الاصطناعي مثل GPT-4.
VITE_AI_MODEL=gpt-4 # نموذج الذكاء الاصطناعي المستخدم (هنا GPT-4).

# Payment Gateway URLs
VITE_STRIPE_API_URL=https://api.stripe.com/v1 # عنوان URL الخاص بـ API لـ Stripe.
VITE_PAYPAL_API_URL=https://api.paypal.com/v1 # عنوان URL الخاص بـ API لـ PayPal.

# Storage Configuration
VITE_S3_BUCKET=your-bucket-name # اسم الدلو (bucket) في خدمة S3 لتخزين الملفات.
VITE_S3_REGION=us-east-1 # المنطقة الجغرافية لخدمة S3.
VITE_S3_URL=https://s3.amazonaws.com/your-bucket # URL للوصول إلى الملفات المخزنة في S3.

# Monitoring
VITE_SENTRY_DSN=https://xxxxx@xxxx.ingest.sentry.io/xxxxx # DSN الخاص بـ Sentry لمراقبة الأخطاء في التطبيق.
VITE_LOGGING_LEVEL=info # مستوى السجلات (log level) الذي يحدد كمية التفاصيل في السجلات.

# Social Media Integration
VITE_FACEBOOK_APP_ID=123456789 # معرف التطبيق الخاص بـ Facebook لدمج تطبيقك مع Facebook.
VITE_GOOGLE_CLIENT_ID=123456789-xxx.apps.googleusercontent.com # معرف العميل الخاص بـ Google لدمج تطبيقك مع Google.
VITE_TWITTER_API_KEY=ABCdef123... # مفتاح API الخاص بـ Twitter لدمج تطبيقك مع Twitter.
